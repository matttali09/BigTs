{
  "name": "bigts",
  "version": "1.0.0",
  "description": "Full stack MERN Application for Scheduling",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "client": "cd bigts && yarn run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd bigts && yarn install",
    "build": "cd bigts && yarn run build",
    "heroku-postbuild": "yarn run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "material-auto-rotating-carousel": "^3.0.2",
    "material-ui": "^0.20.2",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "node-sass": "^4.7.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "react": "^17.0.1",
    "react-awesome-slider": "^4.1.0",
    "react-calendar": "^3.3.1",
    "react-dom": "^17.0.1",
    "react-materialize": "^3.9.7",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "react-swipeable-views": "^0.13.9",
    "shortid": "^2.2.16",
    "web-animation-club": "^0.6.0"
  }
}
